<script>
    import wepy from 'wepy';
    import api from '../../common/api';
    export default class Index extends wepy.page {
        config = {
            navigationBarTitleText: '商标免费查询'
        }
        components = {}
        // mixins = [testMixin]

        data = {
            form: {
                brand_type:'全部类别',
            },
            isShow: false,
            code:'',
            brand_type_array:[
                '全部类别',
                '01 - 化学原料',
                '02 - 颜料油漆',
                '03 - 日化用品',
                '04 - 燃料油脂',
                '05 - 医药',
                '06 - 五金金属',
                '07 - 机械设备',
                '08 - 手工器械',
                '09 - 科学仪器',
                '10 - 医疗器械',
                '11 - 家用电器',
                '12 - 运输工具',
                '13 - 军火烟火',
                '14 - 珠宝钟表',
                '15 - 乐器',
                '16 - 办公文具',
                '17 - 橡胶制品',
                '18 - 皮革皮具',
                '19 - 建筑材料',
                '20 - 家具',
                '21 - 厨房洁具',
                '22 - 绳网袋篷',
                '23 - 纺织纱线',
                '24 - 布料床单',
                '25 - 服装鞋帽',
                '26 - 花边配饰',
                '27 - 地毯席垫',
                '28 - 体育玩具',
                '29 - 食品',
                '30 - 小食配料',
                '31 - 水果花木',
                '32 - 啤酒饮料',
                '33 - 酒',
                '34 - 烟草烟具',
                '35 - 广告贸易',
                '36 - 金融物管',
                '37 - 建筑修理',
                '38 - 通讯传媒',
                '39 - 运输旅行',
                '40 - 材料加工',
                '41 - 教育娱乐',
                '42 - 技术服务',
                '43 - 餐饮酒店',
                '44 - 医疗园艺',
                '45 - 法律服务',
            ]
        }

        computed = {}

        methods = {
            // picker
            bindPickerChange(e) {
                this.form.brand_type=this.brand_type_array[e.detail.value];
            },
            // 获取查询结果
            submitForm() {
                if (this.validate()) {
                    api.checkLogin({isgoback:false}).then(json => {
                        this.form.tokenkey = wx.getStorageSync('tokenkey');
                        this.form.act = 1;
                        this.form.isgoback=false;
                        this.$apply();
                        api.request({
                            url: api.brand_consult,
                            method: 'post',
                            data: this.form
                        }).then(res => {
                            this.isShow=true;
                            this.$apply();
                        }).catch(msg=>{
                            wx.showModal({
                                title: '提示',
                                content: msg,
                                showCancel: false
                            });
                        });
                    });
                }
            },
            // 继续查询
            search() {
                this.form = {};
                this.isShow = false;
                this.$apply();
            },
            // 返回首页
            goIndex() {
                // ?shop_id=${api.shop_id}
                wx.reLaunch({
                    url: `/pages/main/index`
                });
            },
            // 获取手机号码
            getPhoneNumber(e) {
                if(e.detail.errMsg.indexOf('error')>-1){
                    wx.showModal({
                        title: '提示',
                        content: '网络开小差了～',
                        showCancel: false
                    });
                }else {
                    if (this.code) {
                        this.getPhoneNumberRequest(this.code, e);
                    } else {
                        wepy.login()
                            .then(res => {
                                this.getPhoneNumberRequest(res.code, e);
                            })
                    }
                }
            },
            nameInput(e) {
                this.form.brand_name = e.detail.value;
                this.$apply();
            },
            brandGoodInput(e){
                this.form.brand_goods = e.detail.value;
                this.$apply();
            },
            brandTypeInput(e){
                this.form.brand_type = e.detail.value;
                this.$apply();
            },
            mobileInput(e){
                this.form.mobile_phone = e.detail.value;
                this.$apply();
            },
            emailInput(e){
                this.form.email = e.detail.value;
                this.$apply();
            },
            contactInput(e){
                this.form.contact = e.detail.value;
                this.$apply();
            }
        }

        events = {}
        getPhoneNumberRequest(code,e){
            if(e.detail.iv){
                api.request({
                    url: api.bind_wx_phone,
                    data: {
                        get_only:1,
                        code: code,
                        iv: e.detail.iv,
                        encryptedData: e.detail.encryptedData,
                        buid: api.buid
                    },
                    method: 'post'
                }).then(json => {
                    console.log(json);
                    this.code="";
                    if (json.mobile_phone!='') {
                        wx.showToast({
                            title: "获取成功！"
                        });
                        this.form.mobile_phone = json.mobile_phone;
                        this.$apply();
                    }
                }).catch(msg => {
                    this.code="";
                    this.$apply();
                    wx.showModal({
                        title: '提示',
                        content: msg,
                        showCancel: false
                    });
                });
            }

        }
        showToast(content) {
            wx.showToast({
                image: '../../images/fail.png',
                title: content
            });
        }
        // 表单验证
        validate() {
            let {
                    brand_name,
                    mobile_phone
                } = this.form,
                reg = /^1\d{10}$/;
            if (!brand_name) {
                return this.showToast('商标名称不能为空');
            }else if(brand_name.replace(/(^\s+$)/g, ' ')==' '){
                return this.showToast('商标名称不能为空');
            }
            if (!mobile_phone) {
                return this.showToast('手机号不能为空');
            } else {
                if(mobile_phone.replace(/(^\s+$)/g, ' ')==' '){
                    return this.showToast('手机号不能为空');
                }
                if (!reg.test(mobile_phone)) {
                    return this.showToast('手机号格式错误');
                }
            }
            return true;
        }

        onLoad() {
            api.checkLogin().then(json => {
                this.form.tokenkey = wx.getStorageSync('tokenkey');
                this.form.act = 1;
            });
        }
        onShow() {
            api.getBackgroundColor().then(json => {
                let { frontColor, backgroundColor } = json;
                api.setBackgroundColor(frontColor, backgroundColor);
            });
            wepy.login()
                .then(res => {this.code=res.code;this.$apply();});
        }
    }
</script>
