                    <span class="label-tip">商标名称<strong>*</strong></span><input placeholder="请输入需要查询的商标名"  value="{{form.brand_name}}" @input="nameInput" />
                </view>
                <view class="input-txt flex">
                    <span class="label-tip">商品/服务</span><input placeholder="请输入商标名所应用的产品或服务"  value="{{form.brand_goods}}"  @input="brandGoodInput" />
                </view>
                <view class="input-txt flex">
                    <span class="label-tip">商品类别</span>
                    <picker class="boxInput" bindchange="bindPickerChange"  range="{{brand_type_array}}">
                        <view class="picker arrow-icon-d flex">
                            <text>{{form.brand_type?form.brand_type:'请选择商标分类'}}</text>
                            <image src="../../images/property/Triangle.png"/>
                        </view>
                    </picker>
                    <!--<input placeholder="商品类别"  value="{{form.brand_type}}" @input="brandTypeInput" />-->
                </view>
                <view class="input-txt flex" style="min-height:auto !important; margin-bottom:21rpx;">
                    <span class="label-tip"></span>
                    <view class="guardians"><image src="/images/property/guardians.png"/>信息保护中，请放心填写<view class="triangle"></view></view>

                </view>
                <view class="input-txt flex">
                    <span class="label-tip">联系电话<strong>*</strong></span>
                    <input class="mobileStyle" placeholder="免费接收查询结果"  value="{{form.mobile_phone}}" @input="mobileInput" type="number" type="number"/>
                    <button class="weixin" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber"><image src="/images/property/weixin.png"/> 自动填写</button>
                </view>
                <view class="input-txt flex">
                    <span class="label-tip">收件邮箱</span><input placeholder="免费接收查询结果"  value="{{form.email}}" @input="emailInput" />
                </view>
                <view class="input-txt flex">
                    <span class="label-tip">联系人</span><input placeholder="如：莫先生/陈女士"  value="{{form.contact}}" @input="contactInput" />
